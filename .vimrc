filetype plugin indent on
syntax on

set ignorecase
set smartcase
set comments=""
set nolist
set expandtab
set tabstop=4
syntax enable
colorscheme ron

set shiftwidth=4
"set shiftround
"set expandtab
set autoindent
"set ignorecase

nmap <F1> <nop>
imap <F1> <nop>
nmap <Help> <nop>
imap <Help> <nop>

set hls
set nowrap
set ruler

hi MatchParen cterm=bold ctermbg=yellow ctermfg=blue
